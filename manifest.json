{
	"name": "preserveTime",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "A tool for preserve interface",
	"background": {
		"scripts": ["./dist/background.js"],
		"persistent": false
	},
	"content_scripts": [{
		"exclude_globs": [  ],
		"include_globs": [ "*" ],
		"js": [ "content_script.js" ],
		"matches": [ "http://*/*", "https://*/*" ],
		"run_at": "document_end"
	}],
	"devtools_page": "devtools.html",
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}